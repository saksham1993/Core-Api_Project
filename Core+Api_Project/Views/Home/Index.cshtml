@model List< Core_Api_Project.Models.Api_Class>
@{
    ViewData["Title"] = "Home Page";
}

<h1>Display Page</h1>

<table class="table table-bordered table-active table-hover">

    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Mobile</th>
        <th>School</th>
        <th>Address</th>
        <th>Edit</th>
        <th>Delete</th>


    </tr>

    @foreach (var item in Model)
    {

        <tr>

            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Mobile</td>
            <td>@item.School</td>
            <td>@item.Address</td>
            <td><a asp-action="Edit" asp-controller="Home" asp-route-id="@item.Id">Edit</a></td>
            <td><a asp-action="Delete" asp-controller="Home" asp-route-id="@item.Id">Delete</a></td>

        </tr>


    }





</table>